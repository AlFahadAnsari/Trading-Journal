generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Journal {
  id                String   @id @default(cuid())
  createdAt         DateTime @default(now())
  date              DateTime
  totalTrades       String?  
  session           String?
  tradeResult       String?
  profit            String?
  loss              String?
  setup             String?
  reason            String?
  confirmations     String?
  fixedSL           Boolean?
  riskPercent       String?
  rr                String?
  zoneTradeTF       String?
  zoneReason        String?
  confirmTF         String?
  chartAnalysis     String?
  learning          String?
  entryReview       String?
  emotions          String[]
  mistake           Boolean?
  mistakeReason     String?
  avoidPlan         String?
  positive          String?
  improved          String?
  improvementReason String?
  img1              String?
  updateDone        Boolean?
}